/**index.wxss**/
.home{
  height: 100%;
  display: flex;
  flex-direction: column;
}
.home-scroll{
  flex-grow: 1;
}
.home-checkbox{
  padding: 20rpx 12px;
}
.home-content{
  display: flex;
  flex-wrap: wrap;
  padding: 20rpx 20rpx;
  justify-content: space-between;
}
.home-content text:nth-of-type(1){
  padding-left: 20rpx;
  flex-grow: 1;
}
.home-content text:nth-of-type(2){
  align-self: flex-end;
}
